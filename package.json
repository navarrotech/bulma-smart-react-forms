{
  "name": "bulma-smart-react-forms",
  "description": "A lightweight collection of Bulma elements as React components, with built in validators, error handlers, and a smart form",
  "version": "0.0.4-alpha",
  "author": {
    "name": "Alex Navarro",
    "email": "alex@navarrotech.net",
    "url": "https://www.navarrotech.net",
    "organization": "NavarroTech"
  },
  "homepage": "https://github.com/navarrotech/bulma-smart-react-forms",
  "repository": {
    "type": "git",
    "url": "https://github.com/navarrotech/bulma-smart-react-forms"
  },
  "bugs": {
    "url": "https://github.com/navarrotech/bulma-smart-react-forms/issues"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "react",
    "bulma",
    "components",
    "library"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "vite build",
    "postbuild": "npm run bannerize",
    "bannerize": "node scripts/bannerize.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:dev": "DEBUG_PRINT_LIMIT=100000 vitest watch --ui",
    "typecheck:esm": "tsc --project tsconfig.esm.json",
    "typecheck:cjs": "tsc --project tsconfig.cjs.json",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "clean": "rm -rf dist && rm -rf node_modules",
    "reinstall": "yarn clean && yarn install"
  },
  "engines": {
    "node": ">=20.11.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "events": "^3.3.0",
    "fuzzy-search": "^3.2.1",
    "lodash-es": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.2",
    "@storybook/addon-essentials": "8.4.4",
    "@storybook/addon-interactions": "8.4.4",
    "@storybook/addon-onboarding": "8.4.4",
    "@storybook/blocks": "8.4.4",
    "@storybook/react": "8.4.4",
    "@storybook/react-vite": "^8.4.4",
    "@storybook/test": "8.4.4",
    "@stylistic/eslint-plugin-js": "^2.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/fuzzy-search": "^2.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "bulma": "^1.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.11.0",
    "happy-dom": "^14.12.0",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "storybook": "8.4.4",
    "typescript": "^5.6.0",
    "vite": "^5.4.11",
